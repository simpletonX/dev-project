{
  "name": "dev-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "format:scss": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,scss,css,md,json}\"",
    "commit": "commitlint --config commitlint.config.js -e -V"
  },
  "dependencies": {
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
