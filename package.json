{
  "name": "dev-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "format:scss": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,scss,css,md,json}\"",
    "commit": "commitlint --config commitlint.config.js -e -V",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@primevue/themes": "^4.0.7",
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "primelocale": "^1.0.3",
    "primevue": "^4.0.7",
    "vue": "^3.4.37",
    "vue-i18n": "10",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@primevue/auto-import-resolver": "^4.0.7",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.13",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "pnpm": ">=9.0.0",
    "node": ">=21.0.0"
  }
}
